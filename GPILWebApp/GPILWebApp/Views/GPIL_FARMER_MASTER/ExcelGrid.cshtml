@{
    ViewBag.Title = "ExcelGrid";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<h2>ExcelGrid</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()
    
    <div class="row">
        <div class="col-sm-6">
            <div class="sm-3">
                <input type="submit" value="Save" style="width:95px" class="btn btn-success btn-sm" />
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-xs-12">
            <h3 class="header smaller lighter blue">FARMER List</h3>
            <div class="clearfix">
                <div class="pull-right tableTools-container"></div>
            </div>
            <div class="table-header">
                Results for "FARMER DETAILS"
            </div>

            <div class="form-horizondal" id="frmdetl">
                <table id="dynamic-tabl" class="table table-striped table-bordered table-hover alert-dark table-condensed">
                    <thead>
                        <tr>
                            <th>FARM_CODE</th>
                            <th>FARM_CATEGORY</th>
                            <th>FARM_NAME</th>
                            <th>FARM_FATHER_NAME</th>
                            <th>VILLAGE_CODE</th>
                            <th>SOIL_TYPE</th>
                            <th>FARM_ADDRESS1</th>
                            <th>FARM_ADDRESS2</th>
                            <th>MOBILE_NO</th>
                            <th>EMAIL_ID</th>
                            <th>BANK_ACCOUNT_NO</th>
                            <th>BANK_NAME</th>
                            <th>BRANCH_NAME</th>
                            <th>IFSC_CODE</th>
                            <th>CREATED_BY</th>
                            <th>CREATED_DATE</th>
                            <th>STATUS</th>
                            <th></th>
                        </tr>
                    </thead>
                    <tbody id="tbody">
                        @foreach (var c in ViewBag.LCrops)
                        {
                            <tr>
                                <td>@c.FARM_CODE</td>
                                <td>@c.FARM_CATEGORY</td>
                                <td>@c.FARM_NAME</td>
                                <td>@c.FARM_FATHER_NAME</td>
                                <td>@c.VILLAGE_CODE</td>
                                <td>@c.SOIL_TYPE</td>
                                <td>@c.FARM_ADDRESS1</td>
                                <td>@c.FARM_ADDRESS2</td>
                                <td>@c.MOBILE_NO</td>
                                <td>@c.EMAIL_ID</td>
                                <td>@c.BANK_ACCOUNT_NO</td>
                                <td>@c.BANK_NAME</td>
                                <td>@c.BRANCH_NAME</td>
                                <td>@c.IFSC_CODE</td>
                                <td>@c.CREATED_BY</td>
                                <td>@c.CREATED_DATE</td>
                                <td>@c.STATUS</td>
                                <td>
                                    @Html.ActionLink("Edit", "EditExcelGrid", new { id = c.FARM_CODE }) |
                                    @Html.ActionLink("Details", "Details", new { id = c.FARM_CODE }) |
                                    @Html.ActionLink("Delete", "DeleteExcelGrid", new { id = c.FARM_CODE })
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>

            @*<div class="form-horizondal" id="frmdetl">
                <table id="dynamic-tabl" class="table table-striped table-bordered table-hover alert-dark table-condensed">
                  
                </table>
            </div>*@
        </div>
    </div>
}
<div>
    @Html.ActionLink("Back to List", "Index")
</div>


<script src="~/assets/js/jquery-2.1.4.min.js"></script>





<script>

    $(document).ready(function () {
        $('#dynamic-tabl').dataTable(
           );
    });

</script>