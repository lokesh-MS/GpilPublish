@model IEnumerable<GPILWebApp.Models.GPIL_FARMER_MASTER>

@{
    ViewBag.Root = "Master";
    ViewBag.Title = "Farmer Master Cretion";
}
<p>
    <button type="button" role="button" onclick="@("window.location.href='" + @Url.Action("Create", "Farmer") + "'");" class="btn btn-success btn-sm" data-toggle="modal" data-target="#modal-form"> Create New Item </button>
</p>
<div class="table-responsive-sm">
    <table id="dynamic-tables" class="table  table-striped table-bordered table-hover table-condensed">
        <tr>
            <th>
                FARMER CODE
            </th>
            <th>
                FARMER CATEGORY
            </th>
            <th>
                FARMER NAME
            </th>
            <th>
                FARMER FATHER'S NAME
            </th>
            <th>
                VILLAGE CODE
            </th>
            <th>
               SOIL TYPE
            </th>
            <th>
                ADDRESS 1
            </th>
            <th>
               ADDRESS 2
            </th>
            <th>
                ADDRESS 3
            </th>
            <th>
                ADDRESS 4
            </th>
            <th>
               ADDRESS 5
            </th>
            <th>
               ADDRESS 6
            </th>
            <th>
               COUNTRY
            </th>
            <th>
               PIN CODE
            </th>
            <th>
                TELEPHONE NUMBER
            </th>
            <th>
               MOBILE NUMBER
            </th>
            <th>
                EMAIL ID
            </th>
            <th>
               BANK ACCOUNT NUMBER
            </th>
            <th>
               BANK NAME
            </th>
            <th>
               BRANCH NAME
            </th>
            <th>
               IFSC CODE
            </th>
            <th>
                CREATED BY
            </th>
            <th>
              CREATED DATE
            </th>
          
            <th>
               STATUS
            </th>
          
            <th>
               ALERT MESSAGE
            </th>
          
            <th>
                ADHAAR NUMBER
            </th>
            <th>
               LOAN BALANCE
            </th>
          
            <th></th>
        </tr>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.FARM_CODE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FARM_CATEGORY)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FARM_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FARM_FATHER_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.VILLAGE_CODE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.SOIL_TYPE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FARM_ADDRESS1)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FARM_ADDRESS2)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FARM_ADDRESS3)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FARM_ADDRESS4)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FARM_ADDRESS5)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.FARM_ADDRESS6)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.COUNTRY)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PIN_CODE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TEL_NO)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.MOBILE_NO)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EMAIL_ID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BANK_ACCOUNT_NO)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BANK_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BRANCH_NAME)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.IFSC_CODE)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CREATED_BY)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CREATED_DATE)
                </td>
               
                <td>
                    @Html.DisplayFor(modelItem => item.STATUS)
                </td>
               
               
                <td>
                    @Html.DisplayFor(modelItem => item.ALERT_MSG)
                </td>
               
                <td>
                    @Html.DisplayFor(modelItem => item.ATTRIBUTE3)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.ATTRIBUTE4)
                </td>
               
                <td>
                    @Html.ActionLink("Edit", "Edit", new { id = item.FARM_CODE }) |
                    @Html.ActionLink("Details", "Details", new { id = item.FARM_CODE }) |
                    @Html.ActionLink("Delete", "Delete", new { id = item.FARM_CODE })
                </td>
            </tr>
        }

    </table>
</div>
