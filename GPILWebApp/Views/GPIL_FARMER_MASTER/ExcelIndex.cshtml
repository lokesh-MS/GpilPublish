                        <th>Low</th>
                        <th>Others</th>

                        <th>ADD/EDIT/DELETE</th>


                        <th></th>

                    </tr>
                </thead>

                <tbody></tbody>

            </table>



        </div>
    </div>
</div>

<div id="loader">
</div>


<div>
    @Html.ActionLink("Back to List", "Index")
</div>



